{
  "name": "Unpinterest!",
  "version": "0.0.3",
  "description": "Remove pinterest from Google search results",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "*://*/"
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "unpinterested.png",
    "32": "unpinterested.png",
    "48": "unpinterested.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}